    "canvas": "^3.2.1",
    "chalk": "^5.6.2",
    "discord-invite": "^4.0.8",
    "discord.js": "^14.25.1",
    "glob": "^13.0.0",
    "mongoose": "^9.1.5",
    "musicard": "^3.0.1",
    "node-base64-image": "^2.2.0",
    "sharp": "^0.34.5",
    "skia-canvas": "^3.0.8"
